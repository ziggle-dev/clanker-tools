{
  "name": "@ziggler/clanker",
  "version": "0.1.34",
  "description": "AI agents that actually do things - real actions, real changes, real time",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/exports.js",
      "types": "./dist/exports.d.ts"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "clanker": "./dist/index.js"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "dist/tool-manifest.json"
  ],
  "scripts": {
    "build": "tsup && node scripts/generate-tool-manifest.js",
    "dev": "node --import tsx src/index.ts",
    "test:registry": "tsx src/test-registry.ts",
    "start": "node dist/index.js",
    "start:registry": "node dist/index-registry.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "validate-tool": "node scripts/validate-tool-submission.js"
  },
  "keywords": [
    "agent",
    "cli",
    "text-editor",
    "clanker",
    "ai",
    "openaisdk",
    "grok",
    "agentic",
    "claudecode",
    "geminicli",
    "grokcli"
  ],
  "author": "Ziggler",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ziggle-dev/clanker.git"
  },
  "bugs": {
    "url": "https://github.com/ziggle-dev/clanker/issues"
  },
  "homepage": "https://github.com/ziggle-dev/clanker#readme",
  "dependencies": {
    "@types/prismjs": "^1.26.5",
    "@types/react-dom": "19.1.6",
    "@ziggler/clanker": "^0.1.2",
    "axios": "^1.11.0",
    "cfonts": "^3.3.0",
    "chalk": "5.4.1",
    "commander": "14.0.0",
    "dotenv": "17.2.0",
    "enquirer": "^2.4.1",
    "fs-extra": "^11.3.0",
    "immer": "^10.1.1",
    "ink": "6.0.1",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-markdown": "^1.0.4",
    "ink-select-input": "^6.2.0",
    "open": "^10.2.0",
    "openai": "^5.10.2",
    "prismjs": "^1.30.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "ripgrep-node": "^1.0.0",
    "tiktoken": "^1.0.21",
    "uuid": "^11.1.0",
    "valtio": "^2.1.5",
    "zod": "^3.24.1",
    "zog": "^0.0.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "24.1.0",
    "@types/react": "19.1.8",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.31.0",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "preferGlobal": true
}
